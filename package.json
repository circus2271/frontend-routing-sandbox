{
  "name": "routing-player",
  "version": "0.0.1",
  "scripts": {
    "dev": "rimraf ./dist && rimraf ./.parcel-cache && parcel watch src/index.html --no-hmr & live-server --port=8000 ./dist/ --entry-file=index.html",
    "build": "rimraf ./dist && parcel build src/index.html",
    "check": "tsc --noEmit --watch"
  },
  "alias": {
    "@components/*": "./src/scripts/components/$1",
    "@modules/*": "./src/scripts/modules/$1",
    "@custom-elements/*": "./src/scripts/components/custom-elements/$1",
    "@routing/*": "./src/scripts/modules/routing/$1",
    "@types": "./src/scripts/types.ts",
    "@mock-data": "./src/scripts/mock-data.ts"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead, iOS 13",
  "devDependencies": {
    "@parcel/resolver-glob": "^2.9.3",
    "@parcel/transformer-sass": "^2.9.3",
    "@parcel/validator-typescript": "^2.9.3",
    "buffer": "^6.0.3",
    "live-server": "^1.2.2",
    "parcel": "^2.9.3",
    "process": "^0.11.10",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "firebase": "^12.4.0",
    "material-ripple-effects": "^2.0.1"
  }
}
